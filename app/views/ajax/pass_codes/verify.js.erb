<% if @status == 200 %>
  $("#resp-msg").hide();
  $("#passcode-modal").modal("hide");
  var pass_code = "<%= params[:pass_code] %>";
  var meter_url = "<%= ajax_meters_path %>";
  var data = {
    pass_code: pass_code,
    meters: [
      {
        "meter": {
          "pic": "<%= Settings.apartment_pics.house_owner %>",
          "quantity": $("#master-current-index").val(),
          "meter_type": "energy",
          "date": Date(),
        }
      },
      {
        "meter": {
          "pic": "<%= Settings.apartment_pics.son_tran %>",
          "quantity": $("#sub-1-current-index").val(),
          "meter_type": "energy",
          "date": Date(),
        }
      }
    ]
  }
  $.post(meter_url, data, null, 'script');
<% else %>
  $("#resp-msg").show();
  $("#pass-code").addClass("form-control-danger")
    .parent()
    .addClass("has-danger");
  $("#pass-code").focus();
<% end %>
