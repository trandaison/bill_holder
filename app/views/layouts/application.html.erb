<!DOCTYPE html>
<html>
  <head>
    <title>BillHolder</title>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag 'bootstrap.min.css', media: :all %>
    <%= stylesheet_link_tag 'now-ui-kit.css', media: :all %>
    <%= stylesheet_link_tag 'bootstrap-datetimepicker.css', media: :all %>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= stylesheet_link_tag 'main.css' %>
    <%= javascript_include_tag 'application' %>
  </head>
  <body class="index-page sidebar-collapse">
    <div class="wrapper">
      <div class="main">
        <div class="section section-basic">
          <div class="container">
            <%= yield %>
          </div>
        </div>
      </div>
    </div>
    <%= content_for :custom_html %>
  </body>
  <%= javascript_include_tag 'core/jquery.3.2.1.min' %>
  <%= javascript_include_tag 'core/popper.min' %>
  <%= javascript_include_tag 'core/bootstrap.min' %>
  <%= javascript_include_tag 'plugins/bootstrap-switch' %>
  <%= javascript_include_tag 'plugins/nouislider.min' %>
  <%= javascript_include_tag 'plugins/bootstrap-datepicker' %>
  <%= javascript_include_tag 'now-ui-kit.js' %>
  <%= javascript_include_tag 'moment.js' %>
  <%= javascript_include_tag 'moment/vi.js' %>
  <%= javascript_include_tag 'bootstrap-datetimepicker.js' %>
  <%= content_for :custom_js %>
</html>
